{
  "name": "SWD SN UTILS",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "./build/index.html",
    "default_width": 400,
    "default_height": 600
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}